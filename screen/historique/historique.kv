        MDFloatLayout:
            id: tableau_historic
            pos_hint:{'center_x': .355,'center_y': .4}
            size_hint: .71,.75
            radius: 8

        MDLabel:
            id: info
            text: ''
            font_size: 25
            font_name: 'poppins-bold'
            pos_hint:{'center_x':.35}
