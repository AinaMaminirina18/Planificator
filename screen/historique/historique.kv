MDScreen:
    name: 'historique'
    MDFloatLayout:
        orientation:'vertical'
        size_hint: 1,1
        pos_hint:{"center_x":.73,"center_y":.5}

        MDLabel:
            text: 'Historique des traitements '
            font_size: 25
            font_name: 'poppins-bold'
            pos_hint:{'center_x':.5, 'center_y': .95}

        MDLabel:
            text: 'La liste des clients'
            font_size: 20
            font_name: 'poppins-bold'
            pos_hint:{'center_x':.5, 'center_y': .83}

        MDSpinner:
            id: spinner
            size_hint: None, None
